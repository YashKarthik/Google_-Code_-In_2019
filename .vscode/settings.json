{
    "workbench.colorCustomizations": {
        "terminal.background": "#0b0020",
        "terminal.foreground": "#4684cf",
        "tab.unfocusedActiveBorder": "#fff0",
        "bracket-pair-colorizer-2.colors": [
            "Gold",
            "Orchid",
            "LightSkyBlue",
            "Green"
        ],
        "activityBar.activeBorder": "#ee90bb",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#ee90bb",
        "activityBarBadge.foreground": "#0080ff",
        "titleBar.inactiveBackground": "#1857a499",
        "titleBar.inactiveForeground": "#e7e7e799",
        "statusBarItem.hoverBackground": "#1f6fd0"
    },
    "colorTabs.config": [
        {
            "regex": ".*\/mobile\/.*",
            "color": "#FF0000",
            "label": "MOBILE"
        },
        {
            "regex": ".*\/web\/.*",
            "color": "#00FF00",
        },{
            "regex": ".*\/desktop\/.*"
        },
    ],
    "colorTabs.titleBackground": true,
    "colorTabs.ignoreCase": true,
    "python.pythonPath": "/usr/local/bin/python3"
}